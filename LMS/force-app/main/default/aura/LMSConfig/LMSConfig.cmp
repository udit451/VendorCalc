    - Created by Rajat on 3/6/2018.
    -->

<aura:component description="LMSConfig" implements="forceCommunity:availableForAllPageTypes" controller="LMS" access="global">

    <ltng:require scripts="{!join(',',
            $Resource.LMS + '/js/jquery.min.js'
            )}"/>
    
    <aura:attribute name="SuccessMessage" type="String" default="Success!"/>
    <aura:attribute name="ErrorMessage" type="String" default="Something mishappens!"/>
    <aura:attribute name="accountObject" type="Account" default="{'sobjectType' : 'Account'}"/>
    <aura:attribute name="opportunityObject" type="Opportunity" default="{'sobjectType' : 'Opportunity'}"/>
    <aura:attribute name="closedDate" type="String"/>
    <aura:attribute name="CalculatorNameHeader" type="String" default="Calculator"/>
    <aura:attribute name="selectCalculator" type="String" default="Calculator"/>
    <aura:attribute name="TempObjList" type="Object[]"/>
    <aura:attribute name="SavedAccId" type="String"/>
    <aura:attribute name="CheckOpp" type="boolean" default="false"/>
    <aura:attribute name="LoansToConvert" type="Sobject[]" />
    <aura:attribute name="SelectedOpp" type="String" />
    <aura:attribute name="LoanDetails" type="Object" />
    <aura:attribute name="LRO" type="Object" />
    <aura:attribute name="ContractDetails" type="Object" />
    <aura:attribute name="LRepaymentSchedule" type="Object[]" />
    <aura:attribute name="LoanId" type="String" />
    <aura:attribute name="LROId" type="String" />
    <aura:attribute name="isActive" type="String" default="1"/>
    <aura:attribute name="showServCalc" type="boolean" default="false"/>
    <aura:attribute name="showMorgCalc" type="boolean" default="false"/>
    <aura:attribute name="showQuoteCalc" type="boolean" default="false"/>
    <aura:attribute name="showButtons" type="boolean" default="false"/>

    <div class="navbar slds-border_left slds-border_right" style="overflow: hidden;width: 100% !important; z-index: 10000; background: white !important;">
        <div id="Header_Div" class="slds-page-header " style="background-color:lightgray;">
            <div class="slds-media">
                <div class="slds-media__figure ">
                    <img src="{!$Resource.LMS + '/images/LMS_Logo.png'}" style="height: 30px; width: 75px;"/>
                </div>
                <div class="slds-media__body">
                    <h1 class="slds-text-heading_medium slds-m-right_large" style="padding-left: 33%;" title="LMS"><b>Loan Management System</b></h1>
                </div>
            </div>
        </div>

        <div id="spinner_Div" style="display:none;">
            <lightning:spinner aura:id="spinner_Div" variant="brand"/>
        </div>

        <div id="spinnerLMS_Div" style="display:none;">
            <lightning:spinner aura:id="spinnerLMS_Div" variant="brand" />
        </div>

        <div id="Path_Div " class="slds-path slds-p-top_medium slds-p-horizontal_large slds-border_bottom">
            <div class="slds-grid slds-path__track">
                <div class="slds-grid slds-path__scroller-container">
                    <div class="slds-path__scroller" role="application">
                        <div class="slds-path__scroller_inner">
                            <ul class="slds-path__nav" role="listbox" aria-orientation="horizontal">
                                <li class="slds-path__item slds-is-current slds-is-active" role="presentation" onclick="{!c.firstPath}">
                                    <a aria-selected="true" class="slds-path__link" href="javascript:void(0);" id="path-1" role="option" tabindex="0">
                                        <span class="slds-path__stage">
                                            <span class="slds-assistive-text">Stage Complete</span>
                                                </span>
                                        <span class="slds-path__title">Borrower</span>
                                    </a>
                                </li>
                                <li id="secondLi" class="slds-path__item slds-is-incomplete" role="presentation" onclick="{!c.secondPath}">
                                    <a aria-selected="false" class="slds-path__link" href="javascript:void(0);" id="path-2" role="option" tabindex="-1">
                                        <span class="slds-path__stage">
                                            <span class="slds-assistive-text">Stage Complete</span>
                                        </span>
                                        <span class="slds-path__title">Application</span>
                                    </a>
                                </li>
                                <li id="thirdLi" class="slds-path__item slds-is-incomplete" role="presentation" onclick="{!c.thirdPath}">
                                    <a aria-selected="false" class="slds-path__link" href="javascript:void(0);" id="path-3" role="option" tabindex="-1">
                                        <span class="slds-path__stage">
                                            <span class="slds-assistive-text">Current Stage:</span>
                                        </span>
                                        <span class="slds-path__title">{!v.CalculatorNameHeader}</span>
                                    </a>
                                </li>
                                <li id="fourthLi" class="slds-path__item slds-is-incomplete" role="presentation" onclick="{!c.fourthPath}">
                                    <a aria-selected="false" class="slds-path__link" href="javascript:void(0);" id="path-3" role="option" tabindex="-1">
                                        <span class="slds-path__stage">
                                            <span class="slds-assistive-text">Current Stage:</span>
                                        </span>
                                        <span class="slds-path__title"  >Convert To Loan</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div aura:id="SuccessToast" class="slds-backdrop slds-backdrop--open slds-hide">
        <div class="slds-p-top_small" style="height: 4rem;">
            <div class="slds-notify_container slds-is-relative">
                <div class="slds-notify slds-notify_toast slds-theme_success" role="alert">
                    <span class="slds-assistive-text">success</span>
                    <span class="slds-icon_container slds-icon-utility-success slds-m-right_small slds-no-flex slds-align-top" title="Success">
                        <lightning:icon alternativeText="Success" iconName="utility:success" size="small" class="fillclr"/>
                    </span>
                    <div class="slds-notify__content">
                        <h2 class="slds-text-heading_small ">{!v.SuccessMessage}</h2>
                    </div>
                    <button class="slds-button slds-button_icon slds-notify__close slds-button_icon-inverse" title="Close">
                        <lightning:icon alternativeText="Close" iconName="utility:close" size="small" class="fillclr"/>
                        <span class="slds-assistive-text">Close</span>
                    </button>
                </div>
            </div>
        </div>
    </div>

    <div aura:id="ErrorToast" class="slds-backdrop slds-backdrop--open slds-hide">
        <div class="demo-only" style="height: 4rem;">
            <div class="slds-notify_container slds-is-relative">
                <div class="slds-notify slds-notify_toast slds-theme_error" role="alert">
                    <span class="slds-assistive-text">error</span>
                    <span class="slds-icon_container slds-icon-utility-error slds-m-right_small slds-no-flex slds-align-top" title="Error">
                        <lightning:icon alternativeText="Error" iconName="utility:error" size="small" class="fillclr"/>
                    </span>
                    <div class="slds-notify__content">
                        <h2 class="slds-text-heading_small ">{!v.ErrorMessage}</h2>
                    </div>
                    <button class="slds-button slds-button_icon slds-notify__close slds-button_icon-inverse" title="Close">
                        <lightning:icon alternativeText="Close" iconName="utility:close" size="small" class="fillclr"/>
                        <span class="slds-assistive-text">Close</span>
                    </button>
                </div>
            </div>
        </div>
    </div>

    <div id="LMS_Container" class="slds-border_left slds-border_right slds-border_bottom" style="background:transparent;">
        <div id="Borrower_Page" style="background:transparent;">
            <div class="slds-grid slds-wrap slds-p-around_large ">
                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <ui:inputText label="Name" required="true" class="slds-input" aura:id="accountName"
                                  value="{!v.accountObject.Name}" placeholder="eq- John" blur="{!c.borrowerChange}"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <ui:inputURL label="Website" class="slds-input" value="{!v.accountObject.Website}" aura:id="accountEmail" />
                </div>

                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <ui:inputNumber format="0000000000" label="Phone" class="slds-input" value="{!v.accountObject.Phone}" placeholder="-"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <ui:inputText label="Fax" class="slds-input" value="{!v.accountObject.Fax}" placeholder="-"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <ui:inputTextArea label="Address" class="slds-input" value="{!v.accountObject.BillingStreet}"
                                      placeholder="-"/>
                </div>
                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">

                </div>
            </div>
            <div class="slds-grid slds-grid_align-end slds-p-around_large">
                <div class="slds-col slds-p-horizontal_small">
                    <lightning:button onclick="{!c.resetClick}" name="Borrower_Page" label="Reset" variant="destructive"
                                      iconName="utility:close"/>
                </div>
                <div class="slds-col slds-p-horizontal_small">
                    <lightning:button onclick="{!c.saveClick}" name="Borrower_Page" label="Save" variant="brand"
                                      iconName="utility:save"/>
                </div>
                <div class="slds-col slds-p-horizontal_small">
                    <lightning:button onclick="{!c.saveAndNextClick}" name="Borrower_Page" label="Save &amp; Next"
                                      variant="brand" iconName="utility:save"/>
                </div>
                <div class="slds-col slds-p-horizontal_small">
                    <lightning:button onclick="{!c.nextClick}" name="Borrower_Page" label="Next" variant="brand"
                                      iconName="utility:forward"/>
                </div>
            </div>
        </div>

        <div id="Laon_Page" style="background:transparent;display:none;">
            <div class="slds-grid slds-wrap slds-p-around_large">
                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <ui:inputText label="Name" required="true" class="slds-input" aura:id="opportunityName"
                                  value="{!v.opportunityObject.Name}" placeholder="eq- abc" blur="{!c.borrowerChange}"/>
                </div>
                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <c:CustomLookup label="Borrower" lookupId="{!v.opportunityObject.AccountId}" lookupValue=""
                                    lookupObjectAPI="Account" />
                </div>
                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <lightning:input type="date" name="input6" class="DatePicker" label="Term Start Date" value="{!v.opportunityObject.CloseDate}" />
                </div>
                <div class="slds-hide slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <ui:inputSelect class="slds-select" value="{!v.opportunityObject.StageName}" label="Stage" >
                        <ui:inputSelectOption text="Prospecting" label="Prospecting"/>
                        <ui:inputSelectOption text="Settled" label="Settled" value="true"/>
                        <ui:inputSelectOption text="Closed Lost" label="Closed Lost"/>
                    </ui:inputSelect>
                </div>
                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <ui:inputNumber label="Amount" class="slds-input" value="{!v.opportunityObject.Amount}"
                                    placeholder="$"/>
                </div>
                <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_xx-small">
                    <!--<ui:inputNumber format="00000000" label="Application Number" class="slds-input" placeholder="-" value="{!v.opportunityObject.lms2__Application_Number1__c}"/>-->
                    <lightning:input name="appNumber" label="Application Number" placeholder="-" value="{!v.opportunityObject.lms2__Application_Number1__c}" />
                </div>
            </div>
            <div class="slds-grid slds-grid_align-end slds-p-around_large">
                <div class="slds-col slds-p-horizontal_small">
                    <lightning:button onclick="{!c.resetClick}" name="Laon_Page" label="Reset" variant="destructive"
                                      iconName="utility:close"/>
                </div>
                <div class="slds-col slds-p-horizontal_small">
                    <lightning:button onclick="{!c.saveClick}" name="Laon_Page" label="Save" variant="brand"
                                      iconName="utility:save"/>
                </div>
                <div class="slds-col slds-p-horizontal_small">
                    <lightning:button onclick="{!c.saveAndNextClick}" name="Laon_Page" label="Save &amp; Next"
                                      variant="brand" iconName="utility:save"/>
                </div>
                <div class="slds-col slds-p-horizontal_small">
                    <lightning:button onclick="{!c.nextClick}" name="Laon_Page" label="Next" variant="brand"
                                      iconName="utility:forward"/>
                </div>
            </div>
        </div>

        <div id="calculator_Page" style="background:transparent;display:none;">
            <div class="slds-p-top_large slds-size_1-of-4 slds-p-bottom_small slds-p-right_x-large slds-p-left_x-large" style=" overflow: hidden;z-index: 10000;">
                <ui:inputSelect class="slds-select" value="{!v.selectCalculator}" change="{!c.selectCalculator}" >
                    <ui:inputSelectOption text="_Select Calculator_" label="-Select Calculator-"/>
                    <ui:inputSelectOption text="Serviceability Calculator" label="Serviceability Calculator"/>
                    <ui:inputSelectOption text="Mortgage Calculator" label="Mortgage Calculator"/>
                    <ui:inputSelectOption text="Loan Calculator" label="Loan Calculator"/>
                    <ui:inputSelectOption text="Quote Calculator" label="Quote Calculator"/>
                </ui:inputSelect>
            </div>
            <aura:if isTrue="{!v.CheckOpp}" >
                <div id="calcdiv" >
                    <aura:if isTrue="{!v.CalculatorNameHeader == 'Serviceability Calculator'}">
                        <servicecal:SCGeneric_Ext aura:id="SCGeneric_Ext" recordIdMenu="{!v.opportunityObject.Id}"/>
                    </aura:if>
                    <aura:if isTrue="{!v.selectCalculator == 'Mortgage Calculator'}">
                        <MortgageCalc:MortgageCalculator_EXT hasRecordId="{!v.opportunityObject.Id}" aura:id="MortgageCalculator" recordIdString="006" loanAmt="{!v.opportunityObject.Amount}" />
                        <aura:if isTrue="{!v.showButtons}">
                            <div class="slds-grid slds-grid_align-end slds-p-right_medium slds-p-bottom_medium" style="margin-top: -47px;">
                                <div class="slds-col slds-p-horizontal_small">
                                    <lightning:button label="Save" class="" iconName="utility:save" variant="brand" onclick="{!c.saveLoan}"/>
                                </div>
                                <div class="slds-col slds-p-horizontal_small">
                                    <lightning:button label="Save &amp; Next" class="" iconName="utility:save" variant="brand" onclick="{!c.saveNextLoan}"/>
                                </div>
                                <div class="slds-col slds-p-horizontal_small">
                                    <lightning:button label="Next" class="" iconName="utility:forward" variant="brand" onclick="{!c.NextToConvertLoan}"/>
                                </div>
                            </div>
                        </aura:if>
                    </aura:if>
                    <aura:if isTrue="{!v.selectCalculator == 'Loan Calculator'}">
                        <div>
                            <MortgageCalc:LoanCalculator hasRecordId="{!v.opportunityObject.Id}" aura:id="LoanCalculator" recordIdString="006" />
                            <aura:if isTrue="{!v.showButtons}">
                                <div class="slds-grid slds-grid_align-end slds-p-right_medium slds-p-bottom_medium" style="margin-top: -47px;">
                                    <div class="slds-col slds-p-horizontal_small">
                                        <lightning:button label="Save" class="" iconName="utility:save" variant="brand" onclick="{!c.saveLoan}"/>
                                    </div>
                                    <div class="slds-col slds-p-horizontal_small">
                                        <lightning:button label="Save &amp; Next" class="" iconName="utility:save" variant="brand" onclick="{!c.saveNextLoan}"/>
                                    </div>
                                    <div class="slds-col slds-p-horizontal_small">
                                        <lightning:button label="Next" class="" iconName="utility:forward" variant="brand" onclick="{!c.NextToConvertLoan}"/>
                                    </div>
                                </div>
                            </aura:if>
                        </div>
                    </aura:if>
                    <aura:if isTrue="{!v.selectCalculator == 'Quote Calculator'}">
                        <div>
                            <quoteCal:QuoteCalculator aura:id="quote" recordId="{!v.opportunityObject.Id}" amountFinanced="{!v.opportunityObject.Amount}" customerName="{!v.opportunityObject.Name}"/>
                            <aura:if isTrue="{!v.showButtons}">
                                <div class="slds-grid slds-grid_align-end slds-p-right_medium slds-p-bottom_medium slds-p-top_medium" style="margin-top: -47px;">
                                    <div class="slds-col slds-p-horizontal_small">
                                        <lightning:button label="Save" class="" iconName="utility:save" variant="brand" onclick="{!c.saveQuote}"/>
                                    </div>
                                    <div class="slds-col slds-p-horizontal_small">
                                        <lightning:button label="Save &amp; Next" class="" iconName="utility:save" variant="brand" onclick="{!c.saveNextQuote}"/>
                                    </div>
                                    <div class="slds-col slds-p-horizontal_small">
                                        <lightning:button label="Amortization Table" disabled="{!v.monthAmortList.length == 0}" class="" iconName="utility:table" variant="brand" onclick="{!c.showTable}"/>
                                    </div>
                                    <div class="slds-col slds-p-horizontal_small">
                                        <lightning:button label="Reset" class="" iconName="utility:back" variant="destructive" onclick="{!c.resetData}"/>
                                    </div>
                                </div>
                            </aura:if>
                        </div>
                    </aura:if>
                </div>
            </aura:if>
        </div>

        <div id="Convert_To_Loan_Page" style="background:transparent;display:none;" >
            <div  aura:id="AppDiv" style="padding-right: 8%; padding-left: 8%; padding-top: 35px;" class="slds-hide" >
                <table class="slds-table slds-table_fixed-layout slds-table_bordered slds-border_right slds-border_left slds-no-row-hover slds-table_cell-buffer" role="grid">
                    <thead>
                    <tr class="slds-text-title_caps">
                        <th>

                        </th>
                        <th scope="col">
                            <div class="slds-truncate" title="Name"><b>Application Name</b></div>
                        </th>
                        <th scope="col">
<!--                            <div class="slds-truncate" title="Date"><b>Created Date</b></div>-->
                           <div class="slds-truncate" title="Date"><b>Start Date</b></div>
                        </th>
                        <th scope="col">
                            <div class="slds-truncate" title="Amount"><b>Loan Amount</b></div>
                        </th>
                        <th scope="col">
                            <div class="slds-truncate" title="Period"><b>Loan Period</b></div>
                        </th>
                        <th scope="col">
                            <div class="slds-truncate" title="Interest"><b>Final Rate</b></div>
                        </th>
                        <th scope="col">
                            <div class="slds-truncate" title="Interest"><b>Repayment</b></div>
                        </th>
                    </tr>
                    </thead>
                    <tbody>
                    <aura:iteration items="{!v.LoansToConvert}" var="LoanList" indexVar="index">
                        <tr>
                            <td>
                                <!--&lt;!&ndash;<ui:inputRadio name="ToConvertLoan" text="{!LoanList.Id}" click="{!c.handleSelectedOpp}" />&ndash;&gt;-->
                                <lightning:input type="radio" label="" name="ToConvertLoan" value="{!LoanList.Id}" onchange="{!c.handleSelectedOpp}" />
                            </td>
                            <td scope="row">
                                <div class="slds-truncate" title="">{!LoanList.Name}</div>
                            </td>
                            <td>
                                <!--<ui:OutputDateTime format="dd/MM/yyyy" value="{!LoanList.CreatedDate}"/>-->
                                <ui:OutputDateTime format="dd/MM/yyyy" value="{!LoanList.TermStartDate}"/>
                            </td>
                            <td>
                                <div class="slds-truncate" title="">${!LoanList.LoanAmount}</div>
                            </td>
                            <td>
                                <div class="slds-truncate" title="">{!LoanList.LoanPeriod}</div>
                            </td>
                            <td>
                               <!-- <div class="slds-truncate" title="">{!LoanList.Interest}</div>-->
                                <div class="slds-truncate" title="">{!LoanList.FinalRate}%</div>
                            </td>
                            <td>
                               <!-- <div class="slds-truncate" title="">{!LoanList.Interest}</div>-->
                                <div class="slds-truncate" title="">{!LoanList.Repayment}</div>
                            </td>
                        </tr>
                    </aura:iteration>
                    </tbody>
                </table><br/>
                <div  class="slds-align_absolute-center">
                    <lightning:button name="Convert To Loan" label="Convert To Loan" variant="brand" iconName="utility:case" disabled="false" onclick="{!c.convertLoan}"/>
                </div><br/>
            </div>

            <div aura:id="NoAppDiv" class="" >
                <h1 style="margin-left: 29%; color: #FF0000; font-size: 200%;">No Applications found to Convert</h1>
            </div>

            <div class="slds-modal__content slds-p-around_medium " id="modal-content-id-1" >
                <ul class="{!v.showMorgCalc == true ? 'slds-accordion slds-border_right slds-border_left slds-show ':'slds-accordion slds-border_right slds-border_left slds-hide'}" aura:id="MortLoanDetail">
                    <li class="slds-accordion__list-item">
                        <div class="{!v.isActive == '1' ? 'slds-accordion__summary AccActive':'slds-accordion__summary AccNonActive'}"  onclick="{!c.ToggleSectionOne}" aura:id="AccordDivM1" >
                            <h3 style="margin-left:10px;padding:6px;" class="slds-text-heading_small slds-accordion__summary-heading">
                                <lightning:icon class="{!v.isActive == '1' ? 'fillIcon':''}" aura:id="right_icon" iconName="{!v.isActive == '1' ? 'utility:down':'utility:right'}" size="x-small"/>
                                <span class="slds-truncate" style="margin: 0 0.3em;" title="Loan Repayment Schedule">Loan Detail</span>
                            </h3>
                        </div>
                        <section class="{!v.isActive == '1' ? 'slds-accordion__section slds-is-open':'slds-accordion__section slds-hide'}" aura:id="LoanSection"  >
                            <div aria-hidden="false" class="slds-accordion__content" id="accordion-details-01">
                                <table class="slds-table slds-table_fixed-layout slds-no-row-hover slds-table_cell-buffer" role="grid">
                                    <tbody>
                                    <tr>
                                        <td>
