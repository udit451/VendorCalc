<aura:component controller="LMS" implements="force:hasRecordId,flexipage:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" access="global">

    <ltng:require scripts="{!join(',',
                $Resource.LMS + '/js/jquery.min.js'
                )}" />

    <aura:attribute name="recordId" type="String" />
    <aura:handler name="init" action="{!c.doInit}" value="{!this}" />
    <aura:attribute name="LoansToConvert" type="sObject[]" />
    <aura:attribute name="SelectedOpp" type="String" />
    <aura:attribute name="SuccessMessage" type="String" default="Success!"/>
    <aura:attribute name="ErrorMessage" type="String" default="Something mishappens!"/>
    <aura:attribute name="LoanDetails" type="Object" />
    <aura:attribute name="LRO" type="Object" />
    <aura:attribute name="LRepaymentSchedule" type="Object[]" />
    <aura:attribute name="ConvertedSuccessfuly" type="boolean" default="false"/>
    <aura:attribute name="CannotConvertApp" type="boolean" default="false"/>
    <aura:attribute name="CanConvertApp" type="boolean" default="false"/>
    <aura:attribute name="isActive" type="String" default="1"/>
    <aura:attribute name="MessagetoReturn" type="String" />
    <aura:attribute name="ContractDetails" type="Object" />

    <div aura:id="SuccessToast" class="slds-backdrop slds-backdrop&#45;&#45;open slds-hide" style="z-index: 10000;">
        <div class="slds-p-top_small" style="height: 4rem;">
            <div class="slds-notify_container slds-is-relative">
                <div class="slds-notify slds-notify_toast slds-theme_success" role="alert">
                    <span class="slds-assistive-text">success</span>
                    <span class="slds-icon_container slds-icon-utility-success slds-m-right_small slds-no-flex slds-align-top" title="Success">
                        <lightning:icon alternativeText="Success" iconName="utility:success" size="small" class="fillclr"/>
                    </span>
                    <div class="slds-notify__content">
                        <h2 class="slds-text-heading_small ">{!v.SuccessMessage}</h2>
                    </div>
                    <button class="slds-button slds-button_icon slds-notify__close slds-button_icon-inverse" title="Close">
                        <lightning:icon alternativeText="Close" iconName="utility:close" size="small" class="fillclr"/>
                        <span class="slds-assistive-text">Close</span>
                    </button>
                </div>
            </div>
        </div>
    </div>

    <div aura:id="ErrorToast" class="slds-backdrop slds-backdrop&#45;&#45;open slds-hide" style="z-index: 10000;" >
       <div class="demo-only" style="height: 4rem;">
           <div class="slds-notify_container slds-is-relative">
               <div class="slds-notify slds-notify_toast slds-theme_error" role="alert">
                   <span class="slds-assistive-text">error</span>
                   <span class="slds-icon_container slds-icon-utility-error slds-m-right_small slds-no-flex slds-align-top" title="Error">
                       <lightning:icon alternativeText="Error" iconName="utility:error" size="small" class="fillclr"/>
                   </span>
                   <div class="slds-notify__content">
                       <h2 class="slds-text-heading_small ">{!v.ErrorMessage}</h2>
                   </div>
                   <button class="slds-button slds-button_icon slds-notify__close slds-button_icon-inverse" title="Close">
                       <lightning:icon alternativeText="Close" iconName="utility:close" size="small" class="fillclr"/>
                       <span class="slds-assistive-text">Close</span>
                   </button>
               </div>
           </div>
       </div>
    </div>

    <aura:if isTrue="{!v.CannotConvertApp}" >
        <div class="demo-only" style="height: 640px;">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                        <h3 style="font-family: 'Times New Roman';font-size: 30px;" align="center" >{!v.MessagetoReturn}</h3>
                    </div>
                    <footer class="slds-modal__footer">
                        <lightning:button name="Cancel" label="Cancel" variant="destructive" iconName="utility:close" disabled="false" onclick="{!c.returnDetailPage}"/>
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </div>
    </aura:if>

    <aura:if isTrue="{!v.CanConvertApp}" >
        <div class="demo-only" style="height: 640px;">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open slds-modal_medium">
                <div class="slds-modal__container">
                    <header class="slds-modal__header">
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Applications to Convert</h2>
                    </header>
                    <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1" >
                        <table class="slds-table slds-table_fixed-layout slds-table_bordered slds-border_right slds-border_left slds-no-row-hover slds-table_cell-buffer" role="grid">
                            <thead>
                                <tr class="slds-text-title&#45;&#45;caps">
                                    <th>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="Name"><b>Name</b></div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="Date"><b>Created Date</b></div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="Amount"><b>Loan Amount</b></div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="Period"><b>Loan Period</b></div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="Interest"><b>Interest</b></div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <aura:iteration items="{!v.LoansToConvert}" var="LoanList" indexVar="index">
                                    <tr>
                                        <td>
                                            <lightning:input type="radio" label="" name="ToConvertLoan" value="{!LoanList.Id}" onchange="{!c.handleSelectedOpp}" />
                                        </td>
                                        <td scope="row">
                                            <div class="slds-truncate" title="">{!LoanList.Name}</div>
                                        </td>
                                        <td>
                                            <ui:OutputDateTime format="dd/MM/yyyy" value="{!LoanList.CreatedDate}"></ui:OutputDateTime>
                                        </td>
                                        <td>
                                            <div class="slds-truncate" title="">{!LoanList.LoanAmount}</div>
                                        </td>
                                        <td>
                                            <div class="slds-truncate" title="">{!LoanList.LoanPeriod}</div>
                                        </td>
                                        <td>
                                            <div class="slds-truncate" title="">{!LoanList.Interest}</div>
                                        </td>
                                    </tr>
                                </aura:iteration>
                            </tbody>
                        </table><br/>
                    </div>
                    <footer class="slds-modal__footer">
                        <lightning:button name="Convert To Loan" label="Convert To Loan" variant="brand" iconName="utility:case" disabled="false" onclick="{!c.convertLoan}"/>
                        <lightning:button name="Cancel" label="Cancel" variant="destructive" iconName="utility:close" disabled="false" onclick="{!c.returnDetailPage}"/>
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </div>
    </aura:if>

    <aura:if isTrue="{!v.ConvertedSuccessfuly}" >
        <div class="demo-only " style="height: 640px;">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open slds-modal_medium">
                <div class="slds-modal__container">
                    <header class="slds-modal__header">
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Loan Details</h2>
                    </header>
                    <div class="slds-modal__content slds-p-around_medium slds-border--left slds-border--right" id="modal-content-id-1" style="padding-left: 30px;padding-right: 30px;">
                        <ul class="slds-accordion slds-border--right slds-border--left slds-hide" aura:id="MortLoanDetail">
                            <li class="slds-accordion__list-item">
                                <div class="{!v.isActive == '1' ? 'slds-accordion__summary AccActive':'slds-accordion__summary AccNonActive'}"  onclick="{!c.ToggleSectionOne}" aura:id="AccordDivM1" >
                                    <h3 style="margin-left:10px;padding:6px;" class="slds-text-heading_small slds-accordion__summary-heading">
                                        <lightning:icon class="{!v.isActive == '1' ? 'fillIcon':''}" aura:id="right_icon" iconName="{!v.isActive == '1' ? 'utility:down':'utility:right'}" size="x-small"/>
                                        <span class="slds-truncate" style="margin: 0 0.3em;" title="Loan Repayment Schedule">Loan Detail</span>
                                    </h3>
                                </div>
                                <section class="{!v.isActive == '1' ? 'slds-accordion__section slds-is-open':'slds-accordion__section slds-hide'}" aura:id="LoanSection"  >
                                    <div aria-hidden="false" class="slds-accordion__content" id="accordion-details-01">
                                        <table class="slds-table slds-table_fixed-layout slds-no-row-hover slds-table_cell-buffer" role="grid">
                                            <tbody>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan"><B>Loan</B></div>
                                                </td>
                                                <td>
                                                    <div class=" slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan">{!v.LoanDetails.Name}</div>
                                                </td>
                                                <td>
                                                    <div class=" slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Account Name"><B>Account Name</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Account Name">{!v.LoanDetails.AccountName}</div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Application Related To"><B>Application Related To</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Application Related To">{!v.LoanDetails.ApplicationRelatedTo}</div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Current Monthly Loan Repayment"><B>Monthly Loan Repayment</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Current Monthly Loan Repayment"><ui:outputCurrency value="{!v.LoanDetails.CurrentMonthlyLoanRepayment}" /></div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan Account Number"><B>Loan Account Number</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan Account Number">{!v.LoanDetails.LoanAccountNo}</div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Financial Calculator Scenario"><B>Financial Calculator</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Financial Calculator Scenario">{!v.LoanDetails.FinancialCalculatorScenario}</div>
                                                </td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </section>
                            </li>
                            <li class="slds-accordion__list-item">
                                <div class="{!v.isActive == '2' ? 'slds-accordion__summary AccActive':'slds-accordion__summary AccNonActive'}"  onclick="{!c.ToggleSectionTwo}" aura:id="AccordDivM1" >
                                    <h3 style="margin-left:10px;padding:6px;" class="slds-text-heading_small slds-accordion__summary-heading">
                                        <lightning:icon class="{!v.isActive == '2' ? 'fillIcon':''}" aura:id="right_icon" iconName="{!v.isActive == '2' ? 'utility:down':'utility:right'}" size="x-small"/>
                                        <span class="slds-truncate" style="margin: 0 0.3em;" title="Loan Repayment Schedule">Contract Detail</span>
                                    </h3>
                                </div>
                                <section class="{!v.isActive == '2' ? 'slds-accordion__section slds-is-open':'slds-accordion__section slds-hide'}" aura:id="LoanSection"  >
                                    <div aria-hidden="false" class="slds-accordion__content" id="accordion-details-01">
                                        <table class="slds-table slds-table_fixed-layout slds-no-row-hover slds-table_cell-buffer" role="grid">
                                            <tbody>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Contract Number"><B>Contract Number</B></div>
                                                </td>
                                                <td>
                                                    <div class=" slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Contract Number">{!v.ContractDetails.ContractNumber}</div>
                                                </td>
                                                <td>
                                                    <div class=" slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Start Date"><B>Start Date</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Start Date">{!v.ContractDetails.StartDate}</div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="End Date"><B>End Date</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="End Date">{!v.ContractDetails.EndDate}</div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Contract Term"><B>Contract Term(months)</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Contract Term">{!v.ContractDetails.Term}</div>
                                                </td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </section>
                            </li>
                            <li class="slds-accordion__list-item">
                                <div class="{!v.isActive == '3' ? 'slds-accordion__summary AccActive':'slds-accordion__summary AccNonActive'}"  onclick="{!c.ToggleSectionThree}" aura:id="AccordDivM2" >
                                    <h3 style="margin-left:10px;padding:6px;" class="slds-text-heading_small slds-accordion__summary-heading">
                                        <lightning:icon class="{!v.isActive == '3' ? 'fillIcon':''}" aura:id="right_icon" iconName="{!v.isActive == '3' ? 'utility:down':'utility:right'}" size="x-small"/>
                                        <span class="slds-truncate" style="margin: 0 0.3em;" title="Loan Repayment Schedule">Loan Repayment Option</span>
                                    </h3>
                                </div>
                                <section class="{!v.isActive == '3' ? 'slds-accordion__section slds-is-open':'slds-accordion__section slds-hide'}" aura:id="LoanROSection"  >
                                    <div aria-hidden="false" class="slds-accordion__content" id="accordion-details-02">
                                        <table class="slds-table slds-table_fixed-layout slds-no-row-hover slds-table_cell-buffer" role="grid">
                                            <tbody>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan Amount"><B>Loan Amount</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan Amount">{!v.LoanDetails.LoanAmount}</div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan Period"><B>Loan Period</B></div>
                                                </td>
                                                <td>
                                                    <div class=" slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan Period">{!v.LRO.New_Loan_Term_Years}</div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Annual Interest Rate"><B>Annual Interest Rate</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Annual Interest Rate">{!v.LRO.Intrest_Rate_on_new_Loan}</div>
                                                </td>
                                                <td>
                                                    <div class=" slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan Type"><B>Loan Type</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Loan Type">{!v.LRO.Loan_Type}</div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Repayment Type"><B>Repayment Type</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Repayment Type">{!v.LRO.Repayment_Type}</div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Repayment Frequency"><B>Repayment Frequency</B></div>
                                                </td>
                                                <td>
                                                    <div class=" slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Repayment Frequency">{!v.LRO.Repayment_Frequency}</div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Additional Monthly Loan Repayment"><B>Additional Monthly Loan Repayment</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Additional Monthly Loan Repayment">{!v.LRO.AdditionalMonthlyLoanRepayment}</div>
                                                </td>
                                                <td>
                                                    <div class=" slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Annual Interest Rate Sensitivity"><B>Annual Interest Rate Sensitivity</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Annual Interest Rate Sensitivity">{!v.LRO.Annual_Interest_Rate_Sensitivity}</div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Average Annual Inflation Rate"><B>Average Annual Inflation Rate</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Average Annual Inflation Rate">{!v.LRO.AverageAnnual_Inflation_Rate}</div>
                                                </td>
                                                <td>
                                                    <div class=" slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Total Interest"><B>Total Interest</B></div>
                                                </td>
                                                <td>
                                                    <div class="slds-col slds-size_1-of-3 slds-medium-size_4-of-12 slds-large-size_4-of-12" title="Total Interest"><ui:outputCurrency value="{!v.LRO.Total_Interest_over_Loan_Period}" /></div>
                                                </td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </section>
                            </li>
                            <li class="slds-accordion__list-item">
                                <div class="{!v.isActive == '4' ? 'slds-accordion__summary AccActive':'slds-accordion__summary AccNonActive'}"  onclick="{!c.ToggleSectionFour}" aura:id="AccordDivM3" >
                                    <h3 style="margin-left:10px;padding:6px;" class="slds-text-heading_small slds-accordion__summary-heading">
                                        <lightning:icon class="{!v.isActive == '4' ? 'fillIcon':''}" aura:id="right_icon" iconName="{!v.isActive == '4' ? 'utility:down':'utility:right'}" size="x-small"/>
                                        <!--<lightning:icon class="" aura:id="down_icon" iconName="utility:down" size="x-small" />&ndash;&gt;-->
                                        <span class="slds-truncate" style="margin: 0 0.3em;" title="Loan Repayment Schedule">Loan Repayment Schedule</span>
                                    </h3>
                                </div>
                                <section class="{!v.isActive == '4' ? 'slds-accordion__section slds-scrollable slds-is-open':'slds-accordion__section slds-hide'}" aura:id="LoanRSSection" style="height:400px;">
